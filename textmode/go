set -e

DEV="/dev/serial/by-id/usb-FTDI_FT230X_Basic_UART_DO02C6GR-if00-port0"
# python textmode.py $DEV
for F in `cat one passlist`
do
  echo "---------------" $F
  # python reference.py $F
  gforth textmode.fs $F
  # hd out
  diff $F.ref out ||
  ( python cmp.py $F.ref out ; false )
done
wc -w textmode.fs
